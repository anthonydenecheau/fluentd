<source>
  @type forward
  port 24224
  bind 0.0.0.0
</source>

<match *.**>
  @type copy
  <store>
    @type elasticsearch
    host elasticsearch
    port 9200
    logstash_format true
    logstash_prefix fluentd
    #logstash_dateformat %Y-%m-%d_%H:%M:%S.%L%Z
    #time_format %Y-%m-%dT%H:%M:%S.%N
    time_key_format %Y-%m-%dT%H:%M:%S.%N%z
    include_tag_key true
    type_name access_log
    tag_key @log_name
    flush_interval 1s
  </store>
  <store>
    @type stdout
  </store>
</match>

<filter **>
  @type elasticsearch_timestamp_check
  subsecond_precision 3
</filter>